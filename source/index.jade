doctype
html(lang='en-us')
  head
    meta(charset='UTF-8')
    meta(name='viewport' content='width=device-width' initial-scale='1')
    link(href='css/index.css' rel='stylesheet' type='text/css')
  body
    .root#root

      div#test
        h1(v-on:click='toggle') right

      .bar
        .bar-left
          .item
        .bar-center
          .item
        .bar-right
          .item

      .main
        include jade/nav-left.jade
        include jade/side-left.jade

        .center

          a.button.-okay(href='google.com') hello
          a.button.-blue(href='google.com') hello
          a.button.-orange(href='google.com') hello
          a.button.-error(href='google.com') hello
          a.button.-dark(href='google.com') hello

          //- .ad
          include jade/chat.jade

        include jade/side-right.jade
        include jade/nav-right.jade

    script(src='node_modules/vue/dist/vue.min.js')
    script(src='index.js')
